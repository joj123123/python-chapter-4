weight = eval(input("몸무게 입력"))
height1 = eval(input("(피트)키 입력"))
height2 = eval(input("(인치)키 입력"))
height = 12*height1 + height2


Kilograms_per_pound = 0.45359237
Meter_Per_Inch = 0.0254
WeightInKilogram = weight * Kilograms_per_pound
heightInMeters = height * Meter_Per_Inch
bmi = WeightInKilogram / (heightInMeters*heightInMeters)
print(bmi)
if bmi < 18.5:
    print("저체중")
elif bmi < 25:
    print("정상")
elif bmi < 30:
    print("과체중")
else:
    print("비만")
